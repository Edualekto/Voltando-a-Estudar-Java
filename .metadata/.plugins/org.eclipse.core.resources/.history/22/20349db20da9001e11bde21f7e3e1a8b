import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		/*
		String x;
		x = sc.next();
		System.out.println("Você digitou: " + x);
		*/
		
		/*
		int x;
		x = sc.nextInt();
		System.out.println("Você digitou: " + x);
		*/
		
		double x;
		x = sc.nextDouble();
		System.out.println("Você digitou: " + x);
		
		sc.close();
	}

}
